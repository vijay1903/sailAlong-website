<style>
@media (min-width: 1200px)
bootstrap-responsive.css:190
.row-fluid [class*="span"]:first-child {
    margin-left: 0;
}
</style>
{{# each products }}
    <div class="container">
        <div class="row"><br><br><br><br>
            <div class="col-xs-3 item-photo">
                <img style="width:250px;" src="{{this.imgPath}}">
                <h3 style="align-items: center; padding: 0px"><b>{{this.model}}</b></h3>
                <p style="padding: -40px;">
                    <large>
                        <b>Authors: {{this.price}}</b></h4>
                    </large>
                </p>
            </div>
        </div>
    </div>
{{/each}}
